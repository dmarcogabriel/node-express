{
  "name": "node-express",
  "version": "1.0.0",
  "description": "Projeto teste com Node.js e Express",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon",
    "test": "mocha './**/*.spec.js'",
    "lint": "eslint ./src --fix",
    "lint:watch": "esw --quiet -w",
    "log-server": "json-server db.json --port 3004"
  },
  "keywords": [
    "express",
    "node"
  ],
  "author": "Gabriel De Marco",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "eslint-watch": "^6.0.1",
    "express": "4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.4"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "husky": "^4.2.5",
    "json-server": "^0.16.1",
    "lint-staged": "^10.2.0",
    "mocha": "^7.1.0",
    "nodemon": "2.0.2",
    "prettier": "^2.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/*.js": "npm run lint"
  }
}
